    <!-- Bootstrap JS and dependencies -->

    <script>
        document.addEventListener('DOMContentLoaded', function () {
            try {
                var hashValue = window.location.hash;
                if (hashValue !== "") {
                    var id = hashValue.substring(1);
                    var targetElement = document.getElementById(id);
                    if (targetElement) {
                        targetElement.style.marginTop = '110px';
                        targetElement.style.paddingTop = '110px';
                    } else {
                        console.log("Target element with ID '" + id + "' not found.");
                    }
                } else {
                    console.log("No hash value present in the URL.");
                }
            } catch (error) {
                console.error("An error occurred:", error);
            }
        });
    </script>

    <script>

        function appendStyle(id) {
            var refId = localStorage.getItem('refId');

            if (refId !== null && refId !== '') {
                $('#' + refId).css('margin-top', '0px');
                $('#' + refId).css('padding-top', '0px');
                localStorage.removeItem(refId);
            }
            if (refId !== 'about') {
                $('#' + id).css('margin-top', '110px');
                $('#' + id).css('padding-top', '110px');
                localStorage.setItem('refId', id);
            }
        }
    </script>
