/* Dropdown Menu Styles */
.dropdown-menu {
  background-color: #003366;
}
.dropdown-item {
  color: #ffffff;
  transition: background-color 0.3s, color 0.3s;
}
.dropdown-item:hover {
  /* background-color: #0055a5; */
  background-color: #003366;
  color: #ffd700;
}


/* Footer Styles */
.footer {
    background: linear-gradient(135deg, #00274c 0%, #003366 100%); /* Subtle gradient */
    color: #ffffff;
    font-family: 'Roboto', sans-serif; /* Ensure consistency with the rest of the site */
  }
  
  .footer h5 {
    font-weight: 700;
    margin-bottom: 1.5rem;
  }
  
  .footer p, .footer li, .footer a {
    font-size: 0.95rem;
    line-height: 1.6;
  }
  
  .footer a:hover {
    color: #ffd700; /* Gold color on hover */
  }
  
  .footer .social-icons a {
    transition: color 0.3s, transform 0.3s;
  }
  
  .footer .social-icons a:hover {
    color: #ffd700;
    transform: scale(1.2); /* Slightly enlarge icon on hover */
  }
  
  .footer hr {
    margin: 0 1rem;
  }
  
  @media (max-width: 768px) {
    .footer .row > div {
      margin-bottom: 1rem;
    }
  
    .footer .social-icons a {
      margin-right: 1rem;
    }
  }
  
  
  /* Footer Links */
  .footer-link {
    color: rgba(255, 255, 255, 0.8);
    text-decoration: none;
    transition: color 0.3s ease, background-color 0.3s ease;
  }
  
  .footer-link:hover {
    color: #007bff;
    background-color: rgba(255, 255, 255, 0.1);
    padding: 5px;
    border-radius: 5px;
  }
  
  /* Footer Icon Colors */
  .footer-link i {
    margin-right: 8px;
  }
  
  .footer-link:hover i {
    color: #007bff;
  }
  

  .dropdown-submenu {
    position: relative;
  }
  
  .dropdown-submenu > .dropdown-menu {
    top: 0;
    left: 80%;
    margin-left: 0.1rem;
    margin-right: 0.1rem;
  }
  
  /* Ensure the submenu appears on hover */
  .dropdown-submenu:hover > .dropdown-menu {
    display: block;
  }
  
  /* Adjust the positioning on smaller screens */
  @media (max-width: 992px) {
    .dropdown-submenu > .dropdown-menu {
      left: 0;
      margin-left: 0;
      margin-top: 0.1rem;
    }
  }

  .btn-outline-dark {
    color: #fff !important;
    border-color: #fff !important;
    font-size: larger !important;
}

