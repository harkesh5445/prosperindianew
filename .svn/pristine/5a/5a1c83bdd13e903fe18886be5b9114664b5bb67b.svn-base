


.alert1 {
    position: fixed;
    top: 50%;
    left: 70%;
    transform: translate(-50%, -50%);
    padding: 10px;
    border-radius: 5px;
    font-weight: bold;
    z-index: 9999;
    opacity: 0;
    transition: opacity 0.5s;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
}
.success1 {
    color: green;
    background-color: #d4edda;
    border: 1px solid #c3e6cb;
}
.failed1 {
    color: red;
    background-color: #f8d7da;
    border: 1px solid #f5c6cb;
}
.show {
    opacity: 1;
}


